{
  "name": "sol_nuxt",
  "version": "1.0.0",
  "description": "Nästa version av Svenskt översättarlexikon",
  "author": "johan.roxendal@gu.se <johan.roxendal@gu.se>",
  "private": true,
  "dependencies": {
    "@nuxtjs/font-awesome": "^1.0.2",
    "axios": "^0.16.2",
    "axios-debug": "0.0.4",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^4.0.0-beta.2",
    "loader-utils": "^1.1.0",
    "morgan": "^1.9.0",
    "natural-sort": "^1.0.0",
    "nuxt": "^1.0.0",
    "nuxt-generate-cluster": "^1.0.0-gh-c95ed4b",
    "sass-resources-loader": "^1.3.1",
    "vue-template-compiler": "^2.5.13"
  },
  "scripts": {
    "dev": "nuxt --hostname ${DOCKER_HOST:-'0.0.0.0'}",
    "dev2": "node --preserve-symlinks node_modules/nuxt/bin/nuxt-dev",
    "build": "nuxt build",
    "start": "nuxt start --hostname ${DOCKER_HOST:-'0.0.0.0'}",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "debug": "node --inspect node_modules/nuxt/bin/nuxt-dev",
    "spa": "nuxt --spa",
    "test": "docker-compose up --force-recreate --exit-code-from testcafe",
    "cluster-generate": "nuxt-generate -w 4"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3"
  }
}
